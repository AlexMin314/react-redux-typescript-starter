    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-act": "^1.7.4",
    "redux-observable": "^1.0.0",
    "redux-persist": "^5.10.0",
    "rxjs": "^6.3.2",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
