{
  "name": "web_v2",
  "description": "web v2 react app",
  "scripts": {
    "start": "webpack-dev-server --env development:local --config ./config/webpack.config.dev.ts",
    "start:sit": "webpack-dev-server --env development:sit --config ./config/webpack.config.dev.ts",
    "start:uat": "webpack-dev-server --env development:uat --config ./config/webpack.config.dev.ts",
    "start:preprod": "webpack-dev-server --env development:preprod --config ./config/webpack.config.dev.ts",
    "start:prod": "webpack-dev-server --env development:prod --config ./config/webpack.config.dev.ts",
    "build": "webpack --env production --config ./config/webpack.config.prod.ts",
    "build:analysis": "webpack --env production:analysis --config ./config/webpack.config.prod.ts",
    "build:test": "npm run test && npm run build",
    "test": "jest --config ./config/jest/jest.config.json --runInBand",
    "test:commit": "jest --config ./config/jest/jest.config.json --changedSince=master",
    "test:watch": "npm run test -- --watch --updateSnapshot",
    "test:coverage": "npm run clean:test:unit && npm run clean:cache && jest --config ./config/jest/jest.config.json --coverage",
    "test:config": "jest --config ./config/jest/jest.config.json --showConfig",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:open": "npm run test:open:unit",
    "test:open:unit": "opener coverage/lcov-report/index.html",
    "clean": "npm-run-all -p clean:*",
    "clean:cache": "rimraf .cache && rimraf .tmp",
    "clean:build": "rimraf dist",
    "clean:test": "npm-run-all -p clean:test:*",
    "clean:test:unit": "rimraf coverage",
    "postinstall": "npm run clean"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "cuid": "^2.1.4",
