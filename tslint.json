{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "max-line-length": [true, 140],
    "no-console": false,
    "import-name": false,
    "ter-arrow-parens": false,
    "no-this-assignment": [true, {"allow-destructuring": true}],
    "no-boolean-literal-compare": false,
    "variable-name": false,
    "no-duplicate-imports": false,
    "align": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  }
}
